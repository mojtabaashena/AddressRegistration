@model List<AddressRegistration.Data.Entities.Product>
@{ 
    Layout = null;
}
<div class="list-group">
    @foreach (var item in Model)
    {
        <label class="list-group-item">
            <input class="form-check-input me-1"  type="checkbox" value="">
            @item.ProductName  
        </label>
    }
</div>